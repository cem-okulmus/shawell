@prefix sh: <https://www.w3.org/ns/shacl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dbr: <https://dbpedia.org/resource/> .
@prefix dbo: <https://dbpedia.org/ontology/> .


sh:Car1Shape
	a sh:NodeShape ;
	sh:property [			
		sh:path dbo:part ;
		sh:minCount 4 ;
	] .

sh:Car2Shape
	a sh:NodeShape ;
	sh:property [			
		sh:path dbo:part ;
		sh:minCount 5 ;
	] .

sh:CarShape
	a sh:NodeShape ;
	sh:targetClass dbr:Vehicle ;
	sh:and (sh:Car1Shape) ;
	sh:not sh:Car2Shape .


sh:WheelShape
	a sh:NodeShape ;
	sh:targetObjectsOf dbo:part ; 
	sh:property [
		sh:path dbo:shape ;
		sh:hasValue dbr:Circle ; 
	] ;
	sh:property [
		sh:path [
			sh:inversePath dbo:part
		] ;
		sh:node sh:CarShape ;
		sh:maxCount 1 ;
		sh:minCount 1 ;
	] ;
	sh:closed true .